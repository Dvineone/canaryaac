function updateProvince(a_Element, a_Value) {
var l_Select = document.getElementById('province'),
l_Country = a_Element.options[a_Element.selectedIndex].text,
l_Provinces = city_states[l_Country] !== undefined ? city_states[l_Country].split('|') : new Array('----');

while (l_Select.options.length > 0) 
l_Select.options[l_Select.options.length - 1] = null;


for (var i = 0; i < l_Provinces.length; i++) {
if (l_Provinces[i] !== '' && i !== 1) 
l_Select.options[l_Select.options.length] = new Option(l_Provinces[i], l_Provinces[i], false, l_Provinces[i] === a_Value);

}
}
